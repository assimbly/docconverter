<ruleset comparisonMethod="maven"
  xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
  <ignoreVersions>
    <ignoreVersion type="regex">.*(alpha|Alpha|ALPHA).*</ignoreVersion>
    <ignoreVersion type="regex">.*(beta|Beta|BETA).*</ignoreVersion>
    <ignoreVersion type="regex">.*(rc|Rc|RC).*</ignoreVersion>
	<ignoreVersion type="regex">.*[-_\.](alpha|Alpha|ALPHA|b|beta|Beta|BETA|rc|RC|M|EA)[-_\.]?[0-9]*</ignoreVersion>
  </ignoreVersions>
</ruleset>